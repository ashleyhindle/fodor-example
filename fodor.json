{
	"name": "ashleyhindle/fodor-example",
	"install-path": "/var/www/fodor-example/",
	"provisioner": "fodor/provisioner.sh",
	"size": {
		"suggested": "512mb"
	},
	"distro": "ubuntu-14-04-x64",
	"description": "Example Fodor setup which setups a Droplet with Ubuntu 14.04 and nginx",
	"keywords": ["fodor", "example", "nginx"],
	"homepage": "https://github.com/ashleyhindle",
	"inputs": [
		{
			"title": "Fake API Key",
			"name": "fakeapikey",
			"placeholder": "xxxx-xxxx-xxxx-xxxx",
			"type": "regex",
			"regex": "[a-zA-Z\\-0-9]+"
		}
	]
}
